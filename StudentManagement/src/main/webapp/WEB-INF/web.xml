<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>StudentManagement</display-name>
  <welcome-file-list>
  <welcome-file>aad.jsp</welcome-file>
  </welcome-file-list>
   <servlet>
  
  	<servlet-name>StudentController</servlet-name>
  	<display-name>StudentController</display-name>
  	<description></description>
  	<servlet-class>com.student.controller.StudentController</servlet-class>
  </servlet>
   <servlet>
   	<servlet-name>DeleteStudent</servlet-name>
   	<display-name>DeleteStudent</display-name>
   	<description></description>
   	<servlet-class>com.student.controller.DeleteStudent</servlet-class>
   </servlet>
   <servlet>
   	<servlet-name>UpdateStudent</servlet-name>
   	<display-name>UpdateStudent</display-name>
   	<description></description>
   	<servlet-class>com.student.controller.UpdateStudent</servlet-class>
   </servlet>
   <servlet>
   	<servlet-name>a</servlet-name>
   	<display-name>a</display-name>
   	<description></description>
   	<servlet-class>com.student.controller.UpdateStudent</servlet-class>
   </servlet>
  <servlet-mapping>
  	<servlet-name>StudentController</servlet-name>
  	<url-pattern>/saveRequest</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
  	<servlet-name>StudentController</servlet-name>
  	<url-pattern>/ViewList</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>DeleteStudent</servlet-name>
   	<url-pattern>/delete</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>UpdateStudent</servlet-name>
   	<url-pattern>/edit</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>a</servlet-name>
   	<url-pattern>/updateStudentCtl</url-pattern>
   </servlet-mapping>
</web-app>
